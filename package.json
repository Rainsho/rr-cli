{
  "name": "@rainsho/rr-cli",
  "version": "0.0.1",
  "description": "A not cool personal use CLI.",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib"
  ],
  "bin": {
    "rr": "./lib/bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rainsho/rr-cli.git"
  },
  "keywords": [
    "rainsho",
    "typescript",
    "cli"
  ],
  "author": "rainsho rainsho@126.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rainsho/rr-cli/issues"
  },
  "homepage": "https://github.com/Rainsho/rr-cli#readme",
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,md}": [
      "prettier --wirter",
      "git add"
    ]
  }
}
